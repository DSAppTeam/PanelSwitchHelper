<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.effective">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <uses-permission android:name="android.permission.SYSTEM_OVERLAY_WINDOW" />

    <application
        android:name="com.example.demo.App"
        android:allowBackup="true"
        android:icon="@drawable/ic_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.demo.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.demo.scene.chat.ChatActivity"
            android:windowSoftInputMode="stateAlwaysHidden|adjustResize" />
        <activity
            android:name="com.example.demo.scene.chat.ChatSuperActivity"
            android:windowSoftInputMode="stateAlwaysHidden|adjustResize" />
        <activity
            android:name="com.example.demo.scene.chat.ChatFragmentActivity"
            android:windowSoftInputMode="stateAlwaysHidden|adjustResize" />
        <activity
            android:name="com.example.demo.scene.api.ContentActivity"
            android:windowSoftInputMode="stateAlwaysHidden|adjustResize" />
        <activity
            android:name="com.example.demo.scene.api.ResetActivity"
            android:windowSoftInputMode="stateAlwaysHidden|adjustResize" />
        <activity
            android:name="com.example.demo.scene.api.CusPanelActivity"
            android:windowSoftInputMode="stateAlwaysHidden|adjustResize" />
        <activity
            android:name="com.example.demo.scene.api.DefaultHeightPanelActivity"
            android:windowSoftInputMode="stateAlwaysHidden|adjustResize" />
        <activity
            android:name="com.example.demo.scene.video.BiliBiliSampleActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:theme="@style/VideoActivityTheme"
            android:windowSoftInputMode="stateAlwaysHidden|adjustResize" />
        <activity
            android:name="com.example.demo.scene.live.huya.PcHuyaLiveActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:theme="@style/VideoActivityTheme"
            android:windowSoftInputMode="stateAlwaysHidden|adjustResize" />
        <activity
            android:name="com.example.demo.scene.live.douyin.PhoneDouyinLiveActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:theme="@style/VideoActivityTheme"
            android:windowSoftInputMode="stateAlwaysHidden|adjustResize" />
        <activity android:name="com.example.demo.scene.feed.FeedDialogActivity" />
        <activity android:name="com.example.demo.scene.feed.FeedActivity" />
        <activity android:name="com.example.demo.scene.chat.ChatCusContentScrollActivity" />
        <activity android:name="com.example.demo.scene.api.FixIssuesActivity" />
        <activity android:name="com.example.demo.scene.api.FixIssuesActivity2" />
        <activity android:name="com.example.demo.scene.api.FixIssuesActivity4" />
        <activity android:name="com.example.demo.scene.api.FixIssuesForBackBerryActivity" />
        <activity
            android:name="com.example.demo.TestSecondActivity"
            android:exported="false" />
        <activity android:name="com.example.demo.scene.api.FixIssuesActivity3" />
    </application>

</manifest>